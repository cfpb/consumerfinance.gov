{
  "name": "cfgov-refresh",
  "version": "3.0.0-2.4.0",
  "description": "Refreshing consumerfinance.gov.",
  "keywords": [
    "cfpb",
    "consumerfinance.gov",
    "capital-framework"
  ],
  "authors": [
    {
      "name": "Consumer Financial Protection Bureau",
      "email": "tech@cfpb.gov",
      "homepage": "https://cfpb.github.io/"
    }
  ],
  "homepage": "https://github.com/cfpb/cfgov-refresh",
  "repository": {
    "type": "git",
    "url": "https://github.com/cfpb/cfgov-refresh.git"
  },
  "license": "https://github.com/cfpb/cfgov-refresh/blob/master/TERMS.md",
  "dependencies": {
    "capital-framework": "^1.0.0",
    "chosen": "https://github.com/harvesthq/chosen/releases/download/v1.1.0/chosen_v1.1.0.zip",
    "history.js": "~1.8.0",
    "html5shiv": "latest",
    "jquery.easing": "~1.3.0",
    "string_score": "git://github.com/joshaven/string_score#0.1.20",
    "slick-carousel": "~1.3.7",
    "validate": "~0.7.1"
  },
  "exportsOverride": {
    "box-sizing-polyfill": {
      "js": "boxsizing.htc"
    },
    "cf-*": {
      "css": "src/**/*.css",
      "less": "src/**/*.less",
      "fonts": "src/fonts/*.*",
      "js": "src/**/*.js"
    },
    "chosen": {
      "img": "*.png"
    },
    "slick-carousel": {
      "css": ["slick/slick.css", "slick/slick.css.map"],
      "img": "slick/ajax-loader.gif"
    },
    "history.js": {
      "js": "scripts/bundled-uncompressed/html5/jquery.history.js"
    },
    "html5shiv": {
      "js": "dist/html5shiv-printshiv.min.js"
    },
    "string_score": {
      "js": "string_score.js"
    }
  }
}
