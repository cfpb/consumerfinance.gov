{
  "id": "cfgov",
  "instances": 1,
  "mem": 5000,
  "cpus": 5,
  "disk": 2048,
  "uris": [],
  "container": {
    "type": "DOCKER",
    "docker": {
      "image": "{{DOCKER_REGISTRY}}/{{DOCKER_REPO}}:{{DOCKER_IMAGE_TAG}}",
      "network": "BRIDGE",
      "portMappings": [
      {
        "containerPort": 8000,
        "hostPort": 0,
        "servicePort": 84435,
        "protocol": "tcp"
      }]
    }
  },
  "healthChecks": [
  {
    "protocol": "HTTP",
    "portIndex": 0,
    "path": "/",
    "gracePeriod": 5,
    "intervalSeconds": 20,
    "maxConsecutiveFailures": 3
  }],
  "labels": {
    "HAPROXY_GROUP": "internal"
  }
}
